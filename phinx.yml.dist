paths:
    migrations: %%PHINX_CONFIG_DIR%%/{src/*/,vendor/*/*}phinx/migrations
    seeds: %%PHINX_CONFIG_DIR%%/{src/*/,vendor/*/*/}phinx/seeds

environments:
    default_migration_table: phinxlog
    default_database: dev

    dev:
        adapter: mysql
        host: localhost
        name: chatterbot
        user: root
        pass: ''
        port: 3306
        charset: utf8mb4
        collation: utf8mb4_unicode_ci